local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

--[[local Window = Library.CreateLib("JJDI Ikuy", colors)]]
local Window = Library.CreateLib("JJDI Ikuy", "BloodTheme")
local sp = game.Players.LocalPlayer.Character.Humanoid

--[[Section:NewButton("T", "Scritps", function()
    
end)]]



--script
local Tab = Window:NewTab("Scripts")
local Section = Tab:NewSection("SCR")

Section:NewButton("CMD X", "Scritps", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end)
Section:NewButton("Bot Clash", "Scritps", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/BadScripter/Scripts/master/BotClashHub.lua"))()
end)
Section:NewButton("ZEPHYR x", "Scritps", function()
    pcall(function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/ECLIPSEXHUB/ECLIPSE-X/main/ECLIPSE%20X.txt", true))()
end)
end)
Section:NewButton("Remote", "Scritps", function()
    local owner = "Upbolt"
		local branch = "revision"

			local function webImport(file)
    			return loadstring(game:HttpGetAsync(("https://raw.githubusercontent.com/%s/Hydroxide/%s/%s.lua"):format(owner, branch, file)), file .. '.lua')()
	end

	webImport("init")
    webImport("ui/main")
end)

Section:NewButton("DRakDEV", "Scritps", function()
    loadstring(game:HttpGet("https://gist.githubusercontent.com/DinosaurXxX/b757fe011e7e600c0873f967fe427dc2/raw/ee5324771f017073fc30e640323ac2a9b3bfc550/dark%2520dex%2520v4", true))()
end)

Section:NewButton("SAZAhub", "Scritps", function()
    _G.Color = Color3.fromRGB(255, 255, 255)
loadstring(game:HttpGet"https://rawscripts.net/raw/SAZA-HUB_496")()
end)
Section:NewButton("infinity", "Scritps", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)



local Tab = Window:NewTab("Scriptnotrun")
local S2Section = Tab:NewSection("SCR")

S2Section:NewToggle("CMD X", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
    else
        print("Toggle Off")
    end
end)

S2Section:NewToggle("Bot Clash", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
		loadstring(game:HttpGet("https://raw.githubusercontent.com/BadScripter/Scripts/master/BotClashHub.lua"))()
    else
        print("Toggle Off")
    end
end)

S2Section:NewToggle("ZEPHYR x", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
		pcall(function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/ECLIPSEXHUB/ECLIPSE-X/main/ECLIPSE%20X.txt", true))()
end)
    else
        print("Toggle Off")
    end
end)

S2Section:NewToggle("remote", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
		    local owner = "Upbolt"
		local branch = "revision"

			local function webImport(file)
    			return loadstring(game:HttpGetAsync(("https://raw.githubusercontent.com/%s/Hydroxide/%s/%s.lua"):format(owner, branch, file)), file .. '.lua')()
	end

	webImport("init")
    webImport("ui/main")
    else
        print("Toggle Off")
    end
end)

S2Section:NewToggle("Darkdex", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
		loadstring(game:HttpGet("https://gist.githubusercontent.com/DinosaurXxX/b757fe011e7e600c0873f967fe427dc2/raw/ee5324771f017073fc30e640323ac2a9b3bfc550/dark%2520dex%2520v4", true))()
    else
        print("Toggle Off")
    end
end)

S2Section:NewToggle("SAZAhub", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
		    _G.Color = Color3.fromRGB(255, 255, 255)
loadstring(game:HttpGet"https://rawscripts.net/raw/SAZA-HUB_496")()
    else
        print("Toggle Off")
    end
end)

S2Section:NewToggle("infiniyaide", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    else
        print("Toggle Off")
    end
end)
























--option

local Tabb = Window:NewTab("OP")
local lolSection = Tabb:NewSection("OPT")

lolSection:NewSlider("Speed", "fast", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    sp.WalkSpeed = s
end)

lolSection:NewSlider("Jump", "Jumppower", 500, 0, function(j) -- 500 (MaxValue) | 0 (MinValue)
    sp.JumpPower = j
end)

lolSection:NewButton("Position", "Copy", function()
    setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))
end)
lolSection:NewButton("CF", "Copy", function()
    setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame))
end)








--setting

local Tab = Window:NewTab("Setting")
local SettingSection = Tab:NewSection("SEting")

SettingSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
SettingSection:NewButton("FIX BUTTON FLASE", "Scritps", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EarthZ1000/JJ-IKUY/main/file"))()
end)
SettingSection:NewButton("linkUI", "Scritps", function()
    setclipboard("https://xheptcofficial.gitbook.io/kavo-library/")
end)
SettingSection:NewButton("ToggleUI", "Scritps", function()
    Library:ToggleUI()
end)
